        <Label layoutX="483.0" prefHeight="17.0" prefWidth="213.0" text="Application Info">
            <font>
                <Font size="29.0" />
            </font>
        </Label>
        <VBox fx:id="vboxPane" layoutX="410.0" layoutY="43.0" prefHeight="397.0" prefWidth="360.0" />
        <Button layoutX="22.0" layoutY="271.0" mnemonicParsing="false" onAction="#showMailbox" prefHeight="25.0" prefWidth="147.0" text="Open Mailbox" />
    </children>
